
@{
    ViewBag.Title = "AllProject";
    var allProject = ViewBag.AllProject;
}

<div class="container">
    <h3 class="section-title">View All Projects in which you are Involved</h3>
    <hr><br><br>
    
    <div class="row">
        <table id="myTable" class="table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Code Name</th>
                    <th>Status</th>
                    <th>No of Members</th>
                    @*<th>No of Tasks</th>*@
                    <th></th>

                </tr>
            </thead>

            <tbody>
                @foreach (var item in allProject)
                {
                        <tr>
                            <td>@item.Name</td>
                            <td>@item.CodeName</td>
                            <td>@item.Status</td>
                            <td></td>
                            <td>@Html.ActionLink("Update", "UpdateUser", new { id = item.Id }, new { @class = "btn btn-info" })</td>

                        </tr>
                }

            </tbody>
        </table>
    </div>
</div>

